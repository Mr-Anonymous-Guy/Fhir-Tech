# ===========================================
# NAMASTE-SYNC Staging Environment
# ===========================================

# App Configuration
VITE_APP_ENV=staging
VITE_APP_NAME=NAMASTE-SYNC
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DESCRIPTION=FHIR R4 Terminology Service for Indian Traditional Medicine (Staging)

# API Configuration
VITE_API_BASE_URL=https://staging-api.yourdomain.com
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Database Configuration
VITE_MONGODB_URI=mongodb://staging-mongodb:27017/namaste-sync-staging
VITE_MONGODB_DB_NAME=namaste-sync-staging

# Supabase Configuration (Staging)
VITE_SUPABASE_PROJECT_ID=your_staging_project_id
VITE_SUPABASE_PUBLISHABLE_KEY=your_staging_publishable_key
VITE_SUPABASE_URL=https://your-staging-project.supabase.co

# Authentication Configuration
VITE_AUTH_TOKEN_KEY=namaste_auth_token_staging
VITE_AUTH_REFRESH_INTERVAL=3600000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_OFFLINE_MODE=true